{
  "name": "NTMLauncher",
  "version": "1.1.0",
  "description": "NeTaskModded launcher",
  "main": "main.js",
  "dependencies": {
    "decompress-zip": "^0.2.2",
    "electron-dl": "^3.0.2",
    "hastebin": "^0.2.1",
    "minecraft-launcher-core": "^3.15.6",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "distwin": "electron-builder --win",
    "distmac": "electron-builder --mac"
  },
  "build": {
    "appId": "com.mlntcandy.netaskmodded",
    "productName": "NeTaskModded",
    "win": {
      "target": "portable",
      "icon": "build/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "category": "public.app-category.adventure-games"
    }
  },
  "keywords": [],
